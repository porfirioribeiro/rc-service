{
  "name": "rc-service",
  "version": "1.0.0-alpha-1",
  "description": "React Context Services",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "author": "Porfirio Ribeiro<porfirioribeiro@gmail.com>",
  "repository": "https://github.com/porfirioribeiro/rc-service",
  "license": "MIT",
  "files": [
    "dist",
    "es6",
    "src"
  ],
  "mangle": {
    "regex": "^(instances|onServiceUpdate|updateService|renderChild|inject)$"
  },
  "devDependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "^24.0.11",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.12",
    "dot-prop-immutable": "^1.5.0",
    "fast-memoize": "^2.5.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "microbundle": "^0.11.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  },
  "peerDependencies": {
    "react": "^16.8.1"
  },
  "scripts": {
    "clean": "rimraf dist es6 .rts2_cache* sample/.cache sample/dist",
    "test": "jest",
    "build:es6": "tsc -p tsconfig.es6.json",
    "build:mb": "microbundle --target browser",
    "build": "yarn build:es6 && yarn build:mb",
    "example": "parcel sample/index.html",
    "dev": "tsc -p tsconfig.es6.json -w",
    "prepublish": "yarn clean && yarn build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupjest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
