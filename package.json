{
  "name": "rc-service",
  "version": "0.0.11",
  "description": "React Context Services",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "author": "Porfirio Ribeiro<porfirioribeiro@gmail.com>",
  "repository": "https://github.com/porfirioribeiro/rc-service",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "mangle": {
    "regex": "^(instances|onServiceUpdate|getBitMaskForServices|serviceBitMaskMap|observedBits|lastNode|initService|updateService|needsUpdate|renderChild|prevInject)$"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "jest": "^22.4.3",
    "microbundle": "^0.4.4",
    "parcel-bundler": "^1.8.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-testing-library": "^2.5.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "react": "^16.3.1"
  },
  "scripts": {
    "clean": "rimraf dist .cache .rpt2_cache sample/react/dist",
    "test": "jest",
    "build": "microbundle --target browser && microbundle -i src/plugins.ts -o dist/plugins.js",
    "dev": "microbundle -w",
    "sample": "parcel -d sample/react/dist sample/react/index.html",
    "prepublish": "yarn clean && yarn build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
