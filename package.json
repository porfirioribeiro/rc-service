{
  "name": "rc-service",
  "version": "0.0.5",
  "description": "React Context Services",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "author": "Porfirio Ribeiro<porfirioribeiro@gmail.com>",
  "repository": "https://github.com/porfirioribeiro/rc-service",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "mangle": {
    "regex": "^(instances|onServiceUpdate|initService|updateService|servicesToUpdate|prevTo|observedBits|lastNode|renderChild|services|getBitMaskForServices|serviceBitMaskMap)$"
  },
  "devDependencies": {
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "microbundle": "^0.4.4",
    "parcel-bundler": "^1.7.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^16.3.1"
  },
  "scripts": {
    "clean": "rimraf dist .cache .rpt2_cache sample/react/dist",
    "build": "microbundle --target browser && microbundle -i src/plugins.ts -o dist/plugins.js",
    "dev": "microbundle -w",
    "sample": "parcel -d sample/react/dist sample/react/index.html",
    "prepublish": "yarn clean && yarn build"
  }
}
